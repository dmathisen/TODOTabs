{
    "manifest_version": 2,
    "name": "TODOTabs",
    "description": "Create TODO lists as a set of Chrome tabs",
    "homepage_url": "http://danmathisen.com",
    "default_locale": "en",
    "version": "0.0.1",
    "key": "todotabs001",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "browser_action": {
        "default_icon": "icons/icon19.png",
        "default_title": "TODOTabs",
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage",
        "tabs"
    ]
}